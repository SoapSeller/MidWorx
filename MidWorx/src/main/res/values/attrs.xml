<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Define the values for the attribute -->
    <attr name="typeface" format="enum">
        <enum name="agency" value="0"/>
    </attr>

    <!-- Tell Android that the class "CustomTexTView" can be styled,
         and which attributes it supports -->
    <declare-styleable name="CustomTextView">
        <attr name="typeface"/>
        <attr name="is_button" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="BackgroundFun">
        <attr name="tile_image" format="integer"/>
    </declare-styleable>
</resources>